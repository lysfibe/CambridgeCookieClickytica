{{> head }}
<style>
	:root {
		--primary-dark: #1d2129;
		--secondary-dark: #616770; 
		--fb-blue: #4267b2;
		--link-blue: #365899;
		--action-blue: #3578e5;
	}

	* {
		box-sizing: border-box;
		font-family: Arial, Helvetica, sans-serif;
		font-size: 14px;
		transition: all 0.1s ease-out;
	}

	a
	{
		color: var(--link-blue);
		text-decoration: none;
	}

	.site-header
	{
		background-color: #4267b2;
		position: fixed;
		top: 0;
		left: 0;
		right: 0;
		width: 100vw;
		padding: 12px;
		box-shadow: 0 0 1px rgba(0, 0, 0, 0.5);
	}

	.logo-block,
	.logo-block img
	{
		height: 24px;
		width: 24px;
	}

	.title
	{
		margin-left: 10px;
	}

	.title h1
	{
		font-size: 18px;
		line-height: 24px;
		color: #ffffff; 
		padding: 0;
		margin: 0;
	}


	.main
	{
		width: 100vw;
		height: 100vh;
		background-color: #e9ebee;
		padding-top: 80px;
		
	}

	.outer-container
	{
		margin: 0 auto;
		max-width: 1024px;
		width: 100%;
		
		display: flex;
	}

	.outer-container--site-header
	{

	}

	.outer-container--main
	{
		justify-content: space-between;
	}

	.container
	{
		
	}

	.container--cookie
	{
		width: 185px;
	}

	.container--news-feed
	{
		width: 500px;
	}

	.container--multipliers
	{
		width: 310px;
		border: 1px solid #dddfe2;
		background-color: #ffffff;
		border-radius: 5px;
		padding: 12px;
		margin-bottom: 15px;
	}

	.post
	{
		background-color: #ffffff;
		border-radius: 5px;  
		border: 1px solid #dddfe2;
		padding: 12px;
	}

	.post span,
	.post p
	{
		color: var(--primary-dark);
	}

	.post__header
	{
		display: flex;
	}

	.post__avatar
	{
		height: 40px;
		width: 40px;
		border-radius: 50%;
		background-color: var(--action-blue);
		margin-right: 12px;
	}

	.post__usermeta h3
	{
		color: var(--link-blue);
		padding-bottom: 2px;	
		margin: 4px 0 0 0;
	}

	.post__usermeta span
	{
		color: var(--secondary-dark);
		font-size: 12px;
	}

	.post__interactions, .post__reactions
	{
		display: none;
	}

	.stat
	{
		display: inline-block;
		margin-left: 10px;
	}

	.stat > span
	{
		color: #ffffff;
		line-height: 24px;
		font-weight: bold;
	}

	.stat__label
	{
		margin-right: 4px;
	}

	.stat__count
	{
		background-color: rgba(0, 0, 0, 0.4);
		padding: 2px 4px;
		border-radius: 3px;
	}

	.stat-container
	{
		margin-left: auto;
	}

	.footer
	{
		position: fixed;
		bottom: 24px;
		left: 0;
		right: 0;
		text-align: center;
	}

	.post__footer
	{
		text-align: right;
	}

	.cookie-thumb-button
	{
		height: 185px;
		width: 185px;
		border: 1px solid #dddfe2;
		background-color: white;
		border-radius: 5px;
		outline: none;
		cursor: pointer;
	}

	.cookie-thumb-button img
	{
		height: 40px;
		width: 40px;
	}

	.cookie-thumb-button:hover img
	{
		height: 60px;
		width: 60px;
	}

	small a
	{
		font-size: inherit;
	}

	.multipliers__header
	{
		padding: 0;
		margin-top: 0;
		color: var(--secondary-dark);
	}

	.multiplier img
	{
		width: 15px;
		vertical-align: bottom;
	}

	.multiplier__label
	{
		color: var(--action-blue);
		font-weight: bold;
	}

	.multiplier__value
	{
		float: right;
		font-weight: bold;
		color: var(--primary-dark);
	}

	.friend-request
	{
		display: flex;
	}

	.friend-request__info h4
	{
		margin: 0;
		color: var(--link-blue);
	}

	.friend-request__info
	{
		padding-bottom: 18px;
		border-bottom: 1px solid #dddfe2;
		margin-bottom: 18px;
	}

	.friend-request:last-of-type .friend-request__info
	{
		border-bottom: none;
	}

	.friend-request__info p
	{
		color: var(--secondary-dark);
		font-size: 12px;
	}

	.friend-request__info button
	{
		background-color: #4267b2;
		border: 1px solid #29487d;
		border-radius: 2px;
		color: #ffffff;
		padding: 2px 6px 3px;
	}
</style>

